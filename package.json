{
  "name": "tech-arch-doc",
  "version": "2.0.0",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "docs:build": "vuepress-cli build --clean-cache --clean-temp",
    "docs:build-webpack": "DOCS_BUNDLER=webpack pnpm docs:build",
    "docs:clean": "rimraf .vuepress/.temp .vuepress/.cache .vuepress/dist",
    "docs:dev": "vuepress-cli dev --clean-cache --clean-temp",
    "docs:dev-webpack": "DOCS_BUNDLER=webpack pnpm docs:dev",
    "docs:serve": "anywhere -s -h localhost -d .vuepress/dist"
  },
  "dependencies": {
    "@vuepress/bundler-vite": "workspace:*",
    "@vuepress/bundler-webpack": "workspace:*",
    "@vuepress/cli": "workspace:*",
    "@vuepress/client": "workspace:*",
    "@vuepress/core": "workspace:*",
    "@vuepress/plugin-docsearch": "workspace:*",
    "@vuepress/plugin-google-analytics": "workspace:*",
    "@vuepress/plugin-register-components": "workspace:*",
    "@vuepress/plugin-shiki": "^2.0.0-beta.61",
    "@vuepress/theme-default": "workspace:*",
    "@vuepress/utils": "workspace:*",
    "anywhere": "^1.6.0",
    "sass-loader": "^13.2.1",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@vuepress/plugin-back-to-top": "^2.0.0-beta.61",
    "@vuepress/plugin-container": "^2.0.0-beta.61",
    "@vuepress/plugin-medium-zoom": "^2.0.0-beta.61",
    "@vuepress/plugin-nprogress": "^2.0.0-beta.61",
    "@vuepress/plugin-prismjs": "^2.0.0-beta.61",
    "@vuepress/plugin-pwa": "^2.0.0-beta.61",
    "@vuepress/plugin-pwa-popup": "^2.0.0-beta.61",
    "@vuepress/plugin-toc": "^2.0.0-beta.61",
    "vuepress-plugin-clipboard": "^2.1.5",
    "vuepress-plugin-comment2": "^2.0.0-beta.196",
    "vuepress-plugin-copy-code2": "^2.0.0-beta.196",
    "vuepress-plugin-copyright2": "^2.0.0-beta.196",
    "vuepress-plugin-sitemap2": "^2.0.0-beta.196"
  }
}
